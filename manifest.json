{
	"content_scripts": [{
		"js": [ "js/style_injector.js" ],
		"matches": [ "<all_urls>" ]
	}],
	"manifest_version": 2,
	"name": "Style Injector",
	"description": "A handy utility to replace Chrome's removed 'Custom.css' functionality.",
	"version": "0.2.0",
	"icons": {
		"32": "img/si_32.png",
		"64": "img/si_64.png",
	   "128": "img/si_128.png",
	   "256": "img/si_256.png"
	},
	"minimum_chrome_version": "32.0",
	"permissions": [
		"tabs", "storage"
	],
	"options_page": "options.html"
}